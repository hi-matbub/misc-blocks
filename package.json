{
  "name": "misc-blocks",
  "version": "0.0.9",
  "description": "Functional blocks of code written in TypeScript.",
  "main": "./dist/index.js",
  "types": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./find-match-between-two-arrays": "./dist/find-match-between-two-arrays/index.js",
    "./if-array-has-duplicate-values": "./dist/if-array-has-duplicate-values/index.js",
    "./execute-once": "./dist/execute-once/index.js",
    "./sequential-search": "./dist/sequential-search/index.js",
    "./qty-of-matches": "./dist/qty-of-matches/index.js",
    "./find-minimum-value": "./dist/find-minimum-value/index.js"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "npx webpack --config webpack.config.js",
    "tsc": "rm -rf dist && tsc",
    "test": "jest",
    "test:cb": "npm run tsc && npm t && npm run lint:fix",
    "lint": "eslint --ext .js,.ts",
    "lint:fix": "eslint --ext .js,.ts --fix",
    "prepare": "husky install",
    "prepack": "npm run test:cb"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hi-matbub/misc-blocks.git"
  },
  "keywords": [
    "typescript"
  ],
  "author": "hi-matbub",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hi-matbub/misc-blocks/issues"
  },
  "homepage": "https://github.com/hi-matbub/misc-blocks#readme",
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3"
  }
}
